@using StarEnergi.Models;
@model daily_log

@{    
    int? id1 = null;
    int? id2 = null;
    bool isEdit = false;
    bool isEdit2 = false;
    daily_log dl = new daily_log();
    daily_log dl2 = new daily_log();
    List<daily_log_to_wells> logWells = new List<daily_log_to_wells>();
    List<daily_log_to_wells> logWellsNight = new List<daily_log_to_wells>();

    if (ViewBag.mod != null)
    {
        id1 = ViewBag.mod as Nullable<Int32>;
        dl = ViewBag.datas as StarEnergi.Models.daily_log;
        isEdit = true;

        logWells = dl.daily_log_to_wells.ToList();
    }
    if (ViewBag.mod2 != null)
    {
        id2 = ViewBag.mod2 as Nullable<Int32>;
        dl2 = ViewBag.datas2 as StarEnergi.Models.daily_log;
        isEdit2 = true;

        logWellsNight = dl2.daily_log_to_wells.ToList();
    }
    string username = HttpContext.Current.Session["username"] as string;
    string parseValue = "";
    int userShift = 0;

    double a;

    //dynamic wells
    List<daily_log_wells> wells = (List<daily_log_wells>)ViewData["wells"];
    int halfCount = (wells.Count / 2) + (wells.Count % 2);
    List<daily_log_wells> wellsCol1 = wells.Take(halfCount).ToList();
    List<daily_log_wells> wellsCol2 = wells.Skip(halfCount).ToList();
}

