<script type="text/javascript">
    function loadContent() {
        if ('@id2' != '') {
            $('.log-part13').load('DailyLogs/part3?id=@id2', function () {
                $('.log-part3').load('DailyLogs/part3?id=@id1');
            });
            $('.log-part14').load('DailyLogs/part4?id=@id2', function () {
                $('.log-part4').load('DailyLogs/part4?id=@id1');
            });
            $('.log-part16').load('DailyLogs/part6?id=@id2', function () {
                $('.log-part6').load('DailyLogs/part6?id=@id1');
            });
            $('.log-part17').load('DailyLogs/part7?id=@id2', function () {
                $('.log-part7').load('DailyLogs/part7?id=@id1');
            });
            $('.log-part18').load('DailyLogs/part8?id=@id2', function () {
                $('.log-part8').load('DailyLogs/part8?id=@id1');
            });
        } else {
            $('.log-part3').load('DailyLogs/part3?id=@id1');
            $('.log-part4').load('DailyLogs/part4?id=@id1');
            $('.log-part6').load('DailyLogs/part6?id=@id1');
            $('.log-part7').load('DailyLogs/part7?id=@id1');
            $('.log-part8').load('DailyLogs/part8?id=@id1');
        }
    }

    function check_remark(value) {
        switch (value) {
            case 1:
                if (parseFloat('@ViewBag.target_1_sh1') > parseFloat($('#sh1_achievement_1').val())) {
                    $('#sh1_remark_1').removeAttr('disabled');
                } else {
                    $('#sh1_remark_1').attr('disabled','disabled');
                }
                break;
            case 2:
                if (parseFloat('@ViewBag.target_2_sh1') > parseFloat($('#sh1_achievement_2').val())) {
                    $('#sh1_remark_2').removeAttr('disabled');
                } else {
                    $('#sh1_remark_2').attr('disabled', 'disabled');
                }
                break;
            case 3:
                if (parseFloat('@ViewBag.target_1_sh2') > parseFloat($('#sh2_achievement_1').val())) {
                    $('#sh2_remark_1').removeAttr('disabled');
                } else {
                    $('#sh2_remark_1').attr('disabled', 'disabled');
                }
                break;
            case 4:
                if (parseFloat('@ViewBag.target_2_sh2') > parseFloat($('#sh2_achievement_2').val())) {
                    $('#sh2_remark_2').removeAttr('disabled');
                } else {
                    $('#sh2_remark_2').attr('disabled', 'disabled');
                }
                break;
        }
    }

    function approve(shift) {
        var values = {
            is_approve: 1,
            shift: shift
        };

        var ids = shift == 1 ? '@id1' : '@id2';
        values.id = ids;
        $.post("DailyLogs/Approve", values, function (data) {
            console.log(data);
            if (data != "") {
                $('#realmod_save').css("display", "none");
                alert('Change shift Daily Log');
                $('#contentEventLog').load('DailyLogs/addDailyLog/'+'@id1');
            } else {
                $('#realmod_save').css("display", "none");
                alert('Target is not achieving, please submit remark');
            }
        });
    }

    function loadWell(shift) {
        if (isEdit != 'False') {
            if ('@dl' != null) {
                if ('@dl.wma_2_is_text' == '1') {
                    $('input:radio[name="sh1_wma_2_is_text"]').filter('[value="1"]').attr('checked', true);
                    $('input:radio[name="sh1_wma_2_is_text"]').filter('[value="1"]').trigger('change');
                    @if (dl.is_approve == 1)
                    {
                        <text>$('input:radio[name="sh1_wma_2_is_text"]').attr('disabled', 'disabled'); $('#sh1_wma_2_fcv').attr('disabled', 'disabled');</text>
                    }
                    $('#sh1_wma_2_fcv').val('@dl.wma_2_fcv');
                } else {
                    $('input:radio[name="sh1_wma_2_is_text"]').filter('[value="0"]').attr('checked', true);

                    @if (dl.is_approve == 1)
                    {
                        <text>$('input:radio[name="sh1_wma_2_is_text"]').attr('disabled', 'disabled');
                                $('#sh1_wma_2_fcv').attr('disabled', 'disabled');
                                $('#sh1_wma_2_flow').attr('disabled', 'disabled');
                                $('#sh1_wma_2_whp').attr('disabled', 'disabled');</text>
                    }

                    $('#sh1_wma_2_fcv').val('@dl.wma_2_fcv');
                    $('#sh1_wma_2_flow').val('@dl.wma_2_flow');
                    $('#sh1_wma_2_whp').val('@dl.wma_2_whp');
                }
                
                if ('@dl.wma_4_is_text' == '1') {
					$('input:radio[name="sh1_wma_4_is_text"]').filter('[value="1"]').attr('checked', true);
					$('input:radio[name="sh1_wma_4_is_text"]').filter('[value="1"]').trigger('change');
					@if (dl.is_approve == 1)
     {
						<text>$('input:radio[name="sh1_wma_4_is_text"]').attr('disabled', 'disabled'); $('#sh1_wma_4_fcv').attr('disabled', 'disabled');</text>
     }
					$('#sh1_wma_4_fcv').val('@dl.wma_4_fcv');
				} else {
					$('input:radio[name="sh1_wma_4_is_text"]').filter('[value="0"]').attr('checked', true);
					@if (dl.is_approve == 1)
     {
