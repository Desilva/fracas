			                <td class="blue-bg center">Wells</td>
                            <td class="blue-bg center" style="width: 55px;">Input<br>Type</td>
			                <td class="blue-bg center">FCV<br>(%)</td>
			                <td class="blue-bg center">Flow<br>(kg/s)</td>
			                <td class="blue-bg center">WHP<br>(Bar)</td>
			                <td class="blue-bg center">Wells</td>
                            <td class="blue-bg center" style="width: 55px;">Input<br>Type</td>
			                <td class="blue-bg center">FCV<br>(%)</td>
			                <td class="blue-bg center">Flow<br>(kg/s)</td>
			                <td class="blue-bg center">WHP<br>(Bar)</td>
                        </tr>
                @{ 
                    //kamus
                    int i = 0, j = 0; daily_log_to_wells findLogWell; string fcv, flow, whp; byte isText; daily_log_wells well;

                    //algoritma
                    //attributes
                    Dictionary<string, object> isTextAttribute = new Dictionary<string, object>() { { "class", "sh1_is_text" }, { "onchange", "changeInput(this, 1)" } };
                    Dictionary<string, object> fcvAttribute = new Dictionary<string, object>() { { "class", "sh1_fcv" }, { "style", "width: 50px;" } };
                    Dictionary<string, object> flowAttribute = new Dictionary<string, object>() { { "class", "sh1_flow" }, { "style", "width: 50px;" } };
                    Dictionary<string, object> whpAttribute = new Dictionary<string, object>() { { "class", "sh1_whp" }, { "style", "width: 50px;" } };

                    if (dl.is_approve == 1)
                    {
                        isTextAttribute.Add("disabled", "disabled");
                        fcvAttribute.Add("disabled", "disabled");
                        flowAttribute.Add("disabled", "disabled");
                        whpAttribute.Add("disabled", "disabled");
                    }

                    while (i < halfCount)
                    {
                        well = wellsCol1.ElementAt(i);
                        fcv = "";
                        flow = "";
                        whp = "";
                        isText = 0;

                        findLogWell = logWells.Where(m => m.daily_log_wells_id == well.id).FirstOrDefault();
                        if (findLogWell != null)
                        {
                            fcv = findLogWell.fcv;
                            flow = findLogWell.flow;
                            whp = findLogWell.whp;
                            isText = (findLogWell.is_text == null ? (byte)0 : findLogWell.is_text.Value);
                        }
                        <tr>
			                <td>@well.name</td>
                            <td>
                                <input type="hidden" class="sh1_daily_log_wells_id" name="arrWell[@j].daily_log_wells_id" value="@well.id" />
                                @Html.RadioButton("arrWell[" + j + "].is_text", 1, (isText == 1), isTextAttribute)text
                                @Html.RadioButton("arrWell[" + j + "].is_text", 0, (isText == 0), isTextAttribute)input
                            </td>
			                <td class="gray-bg center number">@Html.TextBox("arrWell[" + j + "].fcv", fcv, fcvAttribute)</td>
			                <td class="gray-bg center number">@Html.TextBox("arrWell[" + j + "].flow", flow, flowAttribute)</td>
			                <td class="gray-bg center number">@Html.TextBox("arrWell[" + j + "].whp", whp, whpAttribute)</td>
                    @{                                
                        well = wellsCol2.ElementAtOrDefault(i);
                        fcv = "";
                        flow = "";
                        whp = "";
                        isText = 0;
                        j++;

                        if (well != null)
                        {
                            findLogWell = logWells.Where(m => m.daily_log_wells_id == well.id).FirstOrDefault();
                            if (findLogWell != null)
                            {
                                fcv = findLogWell.fcv;
                                flow = findLogWell.flow;
                                whp = findLogWell.whp;
                                isText = (findLogWell.is_text == null ? (byte)0 : findLogWell.is_text.Value);
                            }
			                <td>@well.name</td>
                            <td>
                                <input type="hidden" class="sh1_daily_log_wells_id" name="arrWell[@j].daily_log_wells_id" value="@well.id" />
                                @Html.RadioButton("arrWell[" + j + "].is_text", 1, (isText == 1), isTextAttribute)text
                                @Html.RadioButton("arrWell[" + j + "].is_text", 0, (isText == 0), isTextAttribute)input
                            </td>
