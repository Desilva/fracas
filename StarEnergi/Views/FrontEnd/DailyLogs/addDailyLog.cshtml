    function bindatch() {
        $.post("DailyLogs/Atch", { id: '@id1' }, function (data) {
            var has = data.files.split(";;");
            $('#atch1').empty();
            for (var f in has) {
                if (has[f] != "") {
                    $('#atch1').append('<a href="' + data.path + has[f] + '">' + has[f] + '</a><br/>');
                }
            }
        });
        if ('@id2' != '') {
            $.post("DailyLogs/Atch", { id: '@id2' }, function (data) {
                var has = data.files.split(";;");
                $('#atch2').empty();
                for (var f in has) {
                    if (has[f] != "") {
                        $('#atch2').append('<a href="' + data.path + has[f] + '">' + has[f] + '</a><br/>');
                    }
                }
            });
        }
    }

