    function save() {
        isSave = true;

        if(!($('#she_obs').is(':checked'))&&!($('#she_ins').is(':checked'))){
            alert('Field Category is required');
            return;
        }

        var equipment_employee = "";
        var checkBoxes = $("input[name='equipment_employee']");
        $.each(checkBoxes, function () {
            if ($(this).attr('checked')) {
                equipment_employee = $(this).val();
            }
        });

        var employee_id = "";
        var checkBoxes = $("input[name='employee']");
        $.each(checkBoxes, function () {
            if ($(this).attr('checked')) {
                employee_id += $(this).val()+",";
            }
        });
        if (validate()) {
            var values = {
                she_obs: $('#she_obs').is(":checked") == true ? 1 : 0,
