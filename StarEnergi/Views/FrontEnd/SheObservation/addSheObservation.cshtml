@{
    int? id = null;
    bool isEdit = false;
    StarEnergi.Models.she_observation dl = null;
    if (ViewBag.mod != null)
    {
        id = ViewBag.mod as Nullable<Int32>;
        dl = ViewBag.datas as StarEnergi.Models.she_observation;
        dl.observer = dl.observer.Split('#').First();
        isEdit = true;
    }
    List<StarEnergi.Models.EmployeeEntity> has = null;
    if (ViewBag.user != null)
    {
        has = ViewBag.user as List<StarEnergi.Models.EmployeeEntity>;
    }
    List<StarEnergi.Models.user_per_role> li = null;
    if (HttpContext.Current.Session["roles"] != null)
    {
        li = HttpContext.Current.Session["roles"] as List<StarEnergi.Models.user_per_role>;
    }
}

