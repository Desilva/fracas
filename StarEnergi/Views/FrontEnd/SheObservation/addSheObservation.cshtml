    function changeInput(radio) {
        if (radio.id == "action_encourages") {
            if (radio.checked) {
                var td = document.getElementById("action_encourage");
                td.disabled = "";
            } else {
                var td = document.getElementById("action_encourage");
                td.disabled = "disabled";
                td.innerHTML = "";
            }
        } else if (radio.id == "action_prevents") {
            if (radio.checked) {
                var td = document.getElementById("action_prevent");
                td.disabled = "";
            } else {
                var td = document.getElementById("action_prevent");
                td.disabled = "disabled";
                td.innerHTML = "";
            }
