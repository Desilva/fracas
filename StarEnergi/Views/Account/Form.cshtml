@model StarEnergi.Models.DutyManagerPresentationStub

@{
    Layout = null;
    ViewBag.Title = "Duty Manager";
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<br/><br />
<fieldset>
    <legend>DUTY MANAGER</legend>
    @Html.HiddenFor(model => model.id_dm)

    <div class="editor-label">
        @Html.LabelFor(model => model.user_id)
    </div>
    <div class="editor-field">
        @Html.DropDownList("delegate_name")
        <div class="errorM"></div>
    </div>
    <div class="editor-label">
        @Html.Label("Delegation Period")
    </div>
    <div class="editor-field">
        @(Html.Telerik().DatePickerFor(model => model.start_date)
            .Value(Model.start_date == null ? DateTime.Today : Model.start_date)
        )
        <div style="padding:5px 10px 0;display:inline-block;text-transform:lowercase">s/d</div>
        @(Html.Telerik().DatePickerFor(model => model.end_date)
                            .Value(Model.end_date == null ? DateTime.Today : Model.end_date)
        )
        <div class="errorM" style="width:250px"></div>
    </div>
    <p>
        <input type="submit" value="Save" onclick="save()"/>
        <input type="submit" value="Back" onclick="showDutyManager()"/>
    </p>
</fieldset>
@section scripts {
<script type="text/javascript">
</script>
}
