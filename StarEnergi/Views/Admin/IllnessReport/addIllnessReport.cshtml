            <tr>
			    <td class="green-bg" style="width:500px">Abdo pain on puffing out or sucking in tummy wall</td>
			    <td style="width:175px">
                    <input type="radio" name="abdo_pain_puffing" value="1" @(isEdit ? dl.abdo_pain_puffing == 1 ? "checked" : "" : "")/>Yes
			    </td>
                <td>&nbsp;</td>
                <td style="width:175px">
                    <input type="radio" name="abdo_pain_puffing" value="0" @(isEdit ? dl.abdo_pain_puffing == 0 ? "checked" : "" : "")/>No
			    </td>
		    </tr>
            <tr>
			    <td class="green-bg" style="width:500px">Areas of tenderness found</td>
			    <td style="width:175px">
                    <input type="radio" name="abdo_area_tenderness_found" value="1" @(isEdit ? dl.abdo_area_tenderness_found == 1 ? "checked" : "" : "")/>Yes
			    </td>
                <td>&nbsp;</td>
                <td style="width:175px">
                    <input type="radio" name="abdo_area_tenderness_found" value="0" @(isEdit ? dl.abdo_area_tenderness_found == 0 ? "checked" : "" : "")/>No
