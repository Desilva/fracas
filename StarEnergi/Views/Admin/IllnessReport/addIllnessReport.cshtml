            <tr>
			    <td class="green-bg no-border-btm no-border-top" style="width:500px"> - Armpit</td>
			    <td style="width:175px">
                    <input type="checkbox" name="ge_gland_armpit_found" id="ge_gland_armpit_found" value="1" @(isEdit ? dl.ge_gland_armpit_found == 1 ? "checked" : "" : "")/>Yes / No
			    </td>
                <td>
                    <input type="checkbox" name="ge_gland_armpit_r" id="ge_gland_armpit_r" value="1" @(isEdit ? dl.ge_gland_armpit_r == 1 ? "checked" : "" : "")/>R
                </td>
                <td style="width:175px">
                    <input type="checkbox" name="ge_gland_armpit_l" id="ge_gland_armpit_l" value="1" @(isEdit ? dl.ge_gland_armpit_l == 1 ? "checked" : "" : "")/>L
