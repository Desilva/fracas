<link rel="stylesheet" type="text/css" href="../../../Content/screen.css" media="screen, projection" />
<link rel="stylesheet" type="text/css" href="../../../Content/style.css" media="screen, projection" />
<script>
    var isEdit = '@isEdit';
    var isSave = false;
    function setIframeHeight(sel, h) {
        $(sel).height(h);
    }

    function changeInput(radio) {
        if (radio.id == "ge_skin_rush") {
            var radios = document.getElementsByName(radio.id);
            var val = -1;
            for (var i = 0, length = radios.length; i < length; i++) {
                if (radios[i].checked) {
                    val = radios[i].value;
                }
            }

            if (val == 1) {
                var td = document.getElementById("ge_skin_rush_size");
                td.disabled = "";
                td = document.getElementById("ge_skin_rush_colour");
                td.disabled = "";
                td = document.getElementById("ge_skin_rush_surface");
                td.disabled = "";
            } else {
                var td = document.getElementById("ge_skin_rush_size");
                td.disabled = "disabled";
                td = document.getElementById("ge_skin_rush_colour");
                td.disabled = "disabled";
                td = document.getElementById("ge_skin_rush_surface");
                td.disabled = "disabled";
            }
        } else if (radio.id == "comment_finding_no") {
            if (radio.checked) {
                var td = document.getElementById("comment_finding");
                td.disabled = "disabled";
            } else {
                var td = document.getElementById("comment_finding");
                td.disabled = "";
            }
        } else if (radio.id == "urine_testing_others") {
            if (radio.checked) {
                var td = document.getElementById("urine_testing_other");
                td.disabled = "";
            } else {
                var td = document.getElementById("urine_testing_other");
                td.disabled = "disabled";
            }
        }
    }

    function save() {
        isSave = true;
        var val = new Array();

        var radios = document.getElementsByName("chest_injury");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[0] = radios[i].value;
            }
        }

        radios = document.getElementsByName("chest_tender_chest_wall");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[1] = radios[i].value;
            }
        }

        radios = document.getElementsByName("chest_position_windpipe_neck");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[2] = radios[i].value;
            }
        }

        radios = document.getElementsByName("chest_movement_breathing");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[3] = radios[i].value;
            }
        }

        radios = document.getElementsByName("chest_breathing_sound");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[4] = radios[i].value;
            }
        }

        radios = document.getElementsByName("chest_air_can_hear");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[5] = radios[i].value;
            }
        }

        radios = document.getElementsByName("abdo_size");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[6] = radios[i].value;
            }
        }

        radios = document.getElementsByName("abdo_pain_coughing");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[7] = radios[i].value;
            }
        }

        radios = document.getElementsByName("abdo_pain_moving");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[8] = radios[i].value;
            }
        }

        radios = document.getElementsByName("abdo_pain_puffing");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[9] = radios[i].value;
            }
        }

        radios = document.getElementsByName("abdo_area_tenderness_found");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[10] = radios[i].value;
            }
        }

        radios = document.getElementsByName("abdo_lumps_swelling_found");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[11] = radios[i].value;
            }
        }

        radios = document.getElementsByName("abdo_bowel_sound");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[12] = radios[i].value;
            }
        }

        radios = document.getElementsByName("ge_colour");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[13] = radios[i].value;
            }
        }

        radios = document.getElementsByName("ge_throat_colour");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[14] = radios[i].value;
            }
        }

        radios = document.getElementsByName("ge_tonsil_size");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[15] = radios[i].value;
            }
        }

        radios = document.getElementsByName("ge_skin_rush");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[16] = radios[i].value;
            }
        }

        radios = document.getElementsByName("post_designation");
        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                val[17] = radios[i].value;
            }
        }

        if (validate()) {
            var values = {
                chest_injury:val[0],
                chest_tender_chest_wall:val[1],
                chest_position_windpipe_neck:val[2],
                chest_movement_breathing:val[3],
                chest_breathing_sound:val[4],
                chest_air_can_hear:val[5],
                abdo_size:val[6],
                abdo_pain_coughing:val[7],
                abdo_pain_moving:val[8],
                abdo_pain_puffing:val[9],
                abdo_area_tenderness_found:val[10],
                abdo_lumps_swelling_found:val[11],
                abdo_bowel_sound: val[12],
                patient_name:$('#patient_name').val(),
                ge_gland_neck_found:$('#ge_gland_neck_found').is(":checked") == true ? 1 : 0,
                ge_gland_neck_r:$('#ge_gland_neck_r').is(":checked") == true ? 1 : 0,
                ge_gland_neck_l:$('#ge_gland_neck_l').is(":checked") == true ? 1 : 0,
                ge_gland_armpit_found:$('#ge_gland_armpit_found').is(":checked") == true ? 1 : 0,
                ge_gland_armpit_r:$('#ge_gland_armpit_r').is(":checked") == true ? 1 : 0,
                ge_gland_armpit_l:$('#ge_gland_armpit_l').is(":checked") == true ? 1 : 0,
                ge_gland_groin_found:$('#ge_gland_groin_found').is(":checked") == true ? 1 : 0,
                ge_gland_groin_r:$('#ge_gland_groin_r').is(":checked") == true ? 1 : 0,
                ge_gland_groin_l:$('#ge_gland_groin_l').is(":checked") == true ? 1 : 0,
                ge_ears:$('#ge_ears').is(":checked") == true ? 1 : 0,
                ge_ears_r:$('#ge_ears_r').is(":checked") == true ? 1 : 0,
                ge_ears_l:$('#ge_ears_l').is(":checked") == true ? 1 : 0,
                ge_colour:val[13],
                ge_eardrum_normal:$('#ge_eardrum_normal').is(":checked") == true ? 1 : 0,
                ge_eardrum_normal_r:$('#ge_eardrum_normal_r').is(":checked") == true ? 1 : 0,
                ge_eardrum_normal_l:$('#ge_eardrum_normal_l').is(":checked") == true ? 1 : 0,
                ge_eardrum_wax:$('#ge_eardrum_wax').is(":checked") == true ? 1 : 0,
                ge_eardrum_wax_r:$('#ge_eardrum_wax_r').is(":checked") == true ? 1 : 0,
                ge_eardrum_wax_l:$('#ge_eardrum_wax_l').is(":checked") == true ? 1 : 0,
                ge_eardrum_red:$('#ge_eardrum_red').is(":checked") == true ? 1 : 0,
                ge_eardrum_red_r:$('#ge_eardrum_red_r').is(":checked") == true ? 1 : 0,
                ge_eardrum_red_l:$('#ge_eardrum_red_l').is(":checked") == true ? 1 : 0,
                ge_eardrum_perforated:$('#ge_eardrum_perforated').is(":checked") == true ? 1 : 0,
                ge_eardrum_perforated_r:$('#ge_eardrum_perforated_r').is(":checked") == true ? 1 : 0,
                ge_eardrum_perforated_l:$('#ge_eardrum_perforated_l').is(":checked") == true ? 1 : 0,
                ge_throat_colour:val[14],
                ge_tonsil_size:val[15],
                ge_skin_rush:val[16],
                ge_skin_rush_size:val[16] == 1 ? $('#ge_skin_rush_size').val() : '',
                ge_skin_rush_colour:val[16] == 1 ? $('#ge_skin_rush_colour').val() : '',
                ge_skin_rush_surface:val[16] == 1 ? $('#ge_skin_rush_surface').val() : '',
                inves_peak:$('#inves_peak').val(),
                urine_testing_blood:$('#urine_testing_blood').is(":checked") == true ? 1 : 0,
                urine_testing_glucose:$('#urine_testing_glucose').is(":checked") == true ? 1 : 0,
                urine_testing_ph:$('#urine_testing_ph').is(":checked") == true ? 1 : 0,
                urine_testing_protein:$('#urine_testing_protein').is(":checked") == true ? 1 : 0,
                urine_testing_sg:$('#urine_testing_sg').is(":checked") == true ? 1 : 0,
                urine_testing_other:$('#urine_testing_others').is(":checked") == true ? $('#urine_testing_other').val() : '',
                comment_finding:$('#comment_finding_no').is(":checked") == true ? $('#comment_finding').val() : '',
                comment_finding_no:$('#comment_finding_no').is(":checked") == true ? 1 : 0,
                possible_diagnosis: $('#possible_diagnosis').val(),
                diag_name: $('#diag_name').val(),
                post_designation:val[17],
                treatment_date: $('#treatment_date').val()
            }

            $('#realmod_save').css("display", "block");
            if (isEdit == 'False') {
                $.post("IllnessReport/Add", values, function (data) {
                    if (data != undefined) {
                        isSave = true;
                        $('#realmod_save').css("display", "none");
                        alert('Illness Report berhasil disimpan');
                        back();
                    } else {
                        $('#realmod_save').css("display", "none");
                        alert('Gagal menambahkan Illness Report');
                    }
                });
            } else if (isEdit == 'True') {
                isSave = true;
                var ids = '@id';
                values.id = ids;
                $.post("IllnessReport/Edit", values, function (data) {
                    if (data != undefined) {
                        $('#realmod_save').css("display", "none");
                        alert('Illness Report berhasil diubah');
                        back();
                    } else {
                        $('#realmod_save').css("display", "none");
                        alert('Gagal mengubah Illness Report');
                    }
                });
            }
    }
}

function validate() {
    return true;
}
</script>
<div class="container">
	<header>
		<div class="page-title" style="width:980px">
			<h1>STAR ENERGY GEOTHERMAL (WAYANG WINDU) LIMITED</h1>
		</div>
		<div class="clearfix" style="width:768px"></div>
		<b>Illness Report - Medical Assessment</b>
	</header>
    <label for="patient_name">Patient Name</label><input id="patient_name" name="patient_name" type="text" value="@(isEdit ? dl.patient_name : "")" />
	<div class="log-part1">
		<table class="w-border" style="width:980px">
			<tr>
				<td class="yellow-bg center" colspan="3"><b>Chest</b></td>
			</tr>
            <tr>
				<td class="yellow-bg center" colspan="3"><b>(bare all chest, front and back)</b></td>
			</tr>
			<tr>
				<td style="width:500px">
                    Sign of injury to chest ?
				</td>
                <td>
                    <input type="radio" name="chest_injury" value="1" @(isEdit ? dl.chest_injury == 1 ? "checked" : "" : "")/>Yes
				</td>
                <td>
                    <input type="radio" name="chest_injury" value="0" @(isEdit ? dl.chest_injury == 0 ? "checked" : "" : "")/>No
				</td>
			</tr>
            <tr>
				<td style="width:500px">
                    Tender chest wall ?
				</td>
                <td>
                    <input type="radio" name="chest_tender_chest_wall" value="1" @(isEdit ? dl.chest_tender_chest_wall == 1 ? "checked" : "" : "")/>Yes
				</td>
                <td>
                    <input type="radio" name="chest_tender_chest_wall" value="0" @(isEdit ? dl.chest_tender_chest_wall == 0 ? "checked" : "" : "")/>No
				</td>
			</tr>
            <tr>
				<td rowspan="2" style="width:500px">
                    Position of windpipe in neck ?
				</td>
                <td rowspan="2">
                    <input type="radio" name="chest_position_windpipe_neck" value="0" @(isEdit ? dl.chest_position_windpipe_neck == 0 ? "checked" : "" : "")/>Central
				</td>
                <td>
                    <input type="radio" name="chest_position_windpipe_neck" value="1" @(isEdit ? dl.chest_position_windpipe_neck == 1 ? "checked" : "" : "")/>To right
				</td>
			</tr>
            <tr>
                <td>
                    <input type="radio" name="chest_position_windpipe_neck" value="2" @(isEdit ? dl.chest_position_windpipe_neck == 2 ? "checked" : "" : "")/>To left
				</td>
			</tr>
            <tr>
				<td rowspan="2" style="width:500px">
                    Chest Movement on breathing
				</td>
                <td>
                    <input type="radio" name="chest_movement_breathing" value="0" @(isEdit ? dl.chest_movement_breathing == 0 ? "checked" : "" : "")/>Relaxed
				</td>
                <td>
                    <input type="radio" name="chest_movement_breathing" value="1" @(isEdit ? dl.chest_movement_breathing == 1 ? "checked" : "" : "")/>Heaving
				</td>
			</tr>
            <tr>
                <td>
                    <input type="radio" name="chest_movement_breathing" value="2" @(isEdit ? dl.chest_movement_breathing == 2 ? "checked" : "" : "")/>Painful
				</td>
                <td>
                    <input type="radio" name="chest_movement_breathing" value="3" @(isEdit ? dl.chest_movement_breathing == 3 ? "checked" : "" : "")/>Unequal
				</td>
			</tr>
            <tr>
				<td rowspan="2" style="width:500px">
                    With your ear to the patient's mouth, their breathing sounds ?
				</td>
                <td rowspan="2">
                    <input type="radio" name="chest_breathing_sound" value="0" @(isEdit ? dl.chest_breathing_sound == 0 ? "checked" : "" : "")/>Normal
				</td>
                <td>
                    <input type="radio" name="chest_breathing_sound" value="1" @(isEdit ? dl.chest_breathing_sound == 1 ? "checked" : "" : "")/>Wheezy in
				</td>
			</tr>
            <tr>
                <td>
                    <input type="radio" name="chest_breathing_sound" value="2" @(isEdit ? dl.chest_breathing_sound == 2 ? "checked" : "" : "")/>Wheezy out
				</td>
			</tr>
            <tr>
				<td rowspan="2" style="width:500px">
                    Using the stathoscope, air can be heard entering
				</td>
                <td rowspan="2">
                    <input type="radio" name="chest_air_can_hear" value="0" @(isEdit ? dl.chest_air_can_hear == 0 ? "checked" : "" : "")/>Both sides of chest
				</td>
                <td>
                    <input type="radio" name="chest_air_can_hear" value="1" @(isEdit ? dl.chest_air_can_hear == 1 ? "checked" : "" : "")/>Right side only
				</td>
			</tr>
            <tr>
                <td>
                    <input type="radio" name="chest_air_can_hear" value="2" @(isEdit ? dl.chest_air_can_hear == 2 ? "checked" : "" : "")/>Left side only
				</td>
			</tr>
		</table>
	</div>
    <br />
