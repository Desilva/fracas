<script type="text/javascript">
    function loadWell() {
        if (isEdit != 'False') {
            if ('@dl' != null) {
                if ('@dl.ge_skin_rush' == '1') {
                    $('input:radio[name="ge_skin_rush"]').filter('[value="1"]').attr('checked', true);
                    $('input:radio[name="ge_skin_rush"]').filter('[value="1"]').trigger('change');
                    $('#ge_skin_rush_size').val('@dl.ge_skin_rush_size');
                    $('#ge_skin_rush_colour').val('@dl.ge_skin_rush_colour');
                    $('#ge_skin_rush_surface').val('@dl.ge_skin_rush_surface');
                } else {
                    $('input:radio[name="ge_skin_rush"]').filter('[value="0"]').attr('checked', true);
                    $('input:radio[name="ge_skin_rush"]').filter('[value="0"]').trigger('change');
                }

                if ('@dl.urine_testing_other' != null && '@dl.urine_testing_other' != '') {
                    $('#urine_testing_others').attr('checked', true);
                    $('#urine_testing_others').trigger('change');
                    $('#urine_testing_other').val('@dl.urine_testing_other');
                } else {
                    $('#urine_testing_others').attr('checked', false);
                    $('#urine_testing_others').trigger('change');
                }

                if ('@dl.comment_finding_no' == '1') {
                    $('#comment_finding_no').attr('checked', true);
                    $('#comment_finding_no').trigger('change');
                } else {
                    $('#comment_finding_no').attr('checked', false);
                    $('#comment_finding_no').trigger('change');
                    $('#comment_finding').val('@dl.comment_finding');
                }
            }
        }
    }

    $(document).ready(function () {
        //resizeIframe();
        loadWell();
    });

    function back() {
        $('#content').load('IllnessReport/Index');
    }
</script>
