@{
    int? id = null;
    bool isEdit = false;
    StarEnergi.Models.she_illness_report dl = new StarEnergi.Models.she_illness_report();
    if (ViewBag.mod != null)
    {
        id = ViewBag.mod as Nullable<Int32>;
        dl = ViewBag.datas as StarEnergi.Models.she_illness_report;
        isEdit = true;
