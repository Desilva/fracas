  <data name="DataTableKBP.SelectCommand" xml:space="preserve">
    <value>with mnth(mnuum)as
(
select 1
union all
select mnuum+1 from mnth where mnuum&lt;12
)
select cr.mnuum as Bulan, DATENAME(month ,cast(CAST(year(getdate()) as varchar(4))+'-'+CAST(cr.mnuum as varchar)+'-01' as datetime)) as NamaBulan, 
	cr.Closed as Closed,
	cr.Raised as Raised,
	oo.[Open] as [Open],
	oo.Overdue as Overdue
from (
select m.mnuum as mnuum,
	COUNT(CASE WHEN p.status = 'VERIFIED' THEN 1 END) as Closed,
	COUNT(CASE WHEN month(p.date_rise) = m.mnuum THEN 1 END) as Raised
from mnth m
left join (Select * from pir where YEAR(date_rise) = @YEAR and [from] = 1) as p on m.mnuum = month(p.date_rise)
where m.mnuum &lt;= (CASE WHEN @YEAR &lt; YEAR(GETDATE()) THEN 12 ELSE 12 END)
    group by m.mnuum) as cr join
(select m.mnuum as mnuum,
	COUNT(CASE WHEN isnull(p.target_completion_init,'') &lt;&gt; '' AND DateDiff(day,GETDATE(),p.target_completion_init) &gt;=0 AND isnull(p.initiator_verified_date,'') = '' THEN 1 END) as [Open],
	COUNT(CASE WHEN isnull(p.target_completion_init,'') &lt;&gt; '' AND DateDiff(day,GETDATE(),p.target_completion_init) &lt; 0 THEN 1 END) as Overdue
from mnth m
left join (Select * from pir where [from] = 1) as p on m.mnuum = month(p.date_rise) where m.mnuum &lt;= (CASE WHEN @YEAR &lt; YEAR(GETDATE()) THEN 12 ELSE 12 END) group by m.mnuum) as oo on oo.mnuum = cr.mnuum

</value>
  </data>
