  <data name="DataTableRekap.SelectCommand" xml:space="preserve">
    <value>SELECT 'Closed' AS [Status], COUNT(CASE WHEN pir.status = 'VERIFIED' THEN 1 END) as yeartodate
   FROM pir
   where YEAR(date_rise) = @YEAR and [from] = 1
UNION ALL
SELECT 'Raised' AS [Status],
        COUNT(CASE WHEN year(pir.date_rise) = @YEAR THEN 1 END) as yeartodate
   FROM pir
   where YEAR(date_rise) = @YEAR and [from] = 1
UNION ALL
SELECT 'Open' AS [Status],
       COUNT(CASE WHEN isnull(pir.target_completion_init,'') &lt;&gt; '' AND DateDiff(day,GETDATE(),pir.target_completion_init) &gt;= 0 AND isnull(pir.initiator_verified_date,'') = '' THEN 1 END) as yeartodate
  FROM pir
  where [from] = 1 AND DATEDIFF(day,GETDATE(),date_rise) &lt;= 0
UNION ALL
SELECT 'Overdue' AS [Status],
       COUNT(CASE WHEN isnull(pir.target_completion_init,'') &lt;&gt; '' AND DateDiff(day,GETDATE(),pir.target_completion_init) &lt; 0 THEN 1 END) as yeartodate
  FROM pir
  where [from] = 1 AND DATEDIFF(day,GETDATE(),date_rise) &lt;= 0</value>
