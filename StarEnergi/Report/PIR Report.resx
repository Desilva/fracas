  <data name="DataTable1.SelectCommand" xml:space="preserve">
    <value>with proc_owner_list(ownlist) as
(
select 'BPL'
union all
select 'GEL'
union all
select 'GDI'
union all
select 'POP'
union all
select 'MTW'
union all
select 'SPE'
union all
select 'OHE'
union all
select 'EPE'
union all
select 'EAI'
union all
select 'OSU'
union all
select 'LCO'
union all
select 'SSU'
union all
select 'MER'
union all
select 'SEC'
union all
select 'SCM'
union all
select 'MIS'
union all
select 'FPR'
union all
select 'PAC'
union all
select 'FHR'
),
procedure_reference_list(procedure_reference) as
(
select (clause_no + ' ' + clause) from pir_clause
)
select a.procedure_reference as reference, proc_owner_list.ownlist as process_owner, COUNT(p.date_rise) as jumlah
from proc_owner_list cross join (select procedure_reference_list.procedure_reference from procedure_reference_list) a
left join (Select * from pir where YEAR(date_rise) = @YEAR and [from] = 1) as p on p.process_owner = proc_owner_list.ownlist and p.procedure_reference = a.procedure_reference
group by ownlist, a.procedure_reference</value>
