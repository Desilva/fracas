  <data name="DataTable2.SelectCommand" xml:space="preserve">
    <value>with procedure_reference_list(proc_ref_list) as
(
select (clause_no + ' ' + clause) from pir_clause
)
SELECT procedure_reference_list.proc_ref_list as procedure_reference, COUNT(pir.date_rise) as yeartodate
from procedure_reference_list
left join (select * from pir where YEAR(date_rise) = @YEAR and [from] = 1 ) as pir on procedure_reference_list.proc_ref_list = pir.procedure_reference
group by proc_ref_list
</value>
